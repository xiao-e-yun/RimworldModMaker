  },
  "bugs": "https://github.com/xiao-e-yun/RimworldModMaker/issues",
  "author": "xiao-e-yun <a0970323059@gmail.com>",
  "dependencies": {
    "better-logging": "^5.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "tsc-alias": "^1.8.16",
    "typedoc": "^0.28.14",
    "typedoc-theme-oxide": "^0.2.0",
    "typescript": "^5.0.0"
  }
